package day14;

public class MakaTest {

	public static void main(String[] args) {
		System.out.println("프로그램의 시작");
		
		Maka m1 = new Maka();//Maka는 래퍼런스 타입의 자료형으로 분류
		m1.color = "파랑";
		m1.makaWrite();
		
		Maka m2 = new Maka();
		m2.makaWrite();
		
		Maka m3 = new Maka();
		m3.makaWrite();
	}

}
