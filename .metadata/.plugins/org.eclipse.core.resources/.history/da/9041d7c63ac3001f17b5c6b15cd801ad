package day13;

public class Day13_05 {

	public static void refTest(int arr[]) {
		
		arr[2]=1004;
		for (int i=0;i<arr.length;i++) {
			System.out.print("arr[i]"+arr[i]);
		}
		System.out.println();
	}
	public static void main(String[] args) {


		int arr[] = {10, 30, 40, 50};
		
		for(int i =0; i<arr.length;i++) {
			System.out.print("arr[i]:"+arr[i]);
		}
		System.out.println();
		
		refTest(arr);

	}

}
