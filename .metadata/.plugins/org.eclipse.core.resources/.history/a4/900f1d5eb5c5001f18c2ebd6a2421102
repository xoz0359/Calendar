package test;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner input=new Scanner(System.in);
		
		int A = input.nextInt();
		int B = input.nextInt();
		
		int arr[] = new int[A];
		
		for (int i = 0; i<B; i++) {
			int C = input.nextInt();
			int D = input.nextInt();
			int k = D;
			for (int j = C-1; j < D; j++) {
				arr[j] = k;
				k--;
			}
		}
	}
}