package test;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner input=new Scanner(System.in);
		int arr[] = new int [10];
		
		for (int i = 0; i < arr.length; i++) {
			int A = input.nextInt();
			arr[i] = A%42;
		}
		
		for (int i=0; i<arr.length; i++) {
			for(int j=i+1; j < arr.length; j++) {
				if (arr[i] == -1) {
					break;
				}
				if (arr[i] == arr[j]){// 주어진 입력값으로 -1은 나머지가 될 수 없으므로 중복값 표시에 사용
					arr[j] = -1;
				}
			}
		}
		int cnt = 0;
		for (int i=0; i<arr.length; i++) {
			if (arr[i]!= -1) {
				cnt++;
			}
		}
		System.out.println(cnt);
	}
}