package day08;

public class Day08_02 {

	public static void main(String[] args) {
		for(int i=0;i<5;i++) {	
			System.out.println("O O O O O");
		}
		System.out.println("--------------------");
		for(int i=0;i<5;i++) {
			for(int j=i;j<5;j++) {
				System.out.print("O ");
			}
			System.out.println();
		}
	}

}
