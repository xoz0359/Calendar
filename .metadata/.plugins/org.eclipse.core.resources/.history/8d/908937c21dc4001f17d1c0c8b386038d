package day14;

public class Car {
	String who;
	String name;
	String color;
	int wheel;
	
	public void getInfo() {
		System.out.println(who);
		System.out.println(name);
		System.out.println(color);
		System.out.println(wheel);
	}
	
	public void goFron() {
		System.out.println(who+"의 "+ wheel+"륜 "+color+name+", 전진합니다, 부웅~");
	}

	
	public void goBack() {
		System.out.println(who+"의 "+ wheel+"륜 "+color+name+", 후진합니다, 부웅~");
	}
}
