package test;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner input=new Scanner(System.in);
		int A = input.nextInt();
		for (int i=1;i<=A;i++) {
			int B = input.nextInt();
			int C = input.nextInt();
			int sum = 0;
			sum = B+C;
			System.out.println("Case #"+i+":"+sum);
		}

	}
}