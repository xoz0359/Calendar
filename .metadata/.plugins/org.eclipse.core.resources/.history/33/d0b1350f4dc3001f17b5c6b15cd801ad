package day13;

import java.io.*;
public class Day13_05_2 {

	public static void sFactor(int input) {
		for (int i=1;i<=9;i++) {
			System.out.println(input+"*"+i+"="+input*i);
		}
	}
	public static void main(String[] args) 
	throws IOException{
		// 사용자로부터 단을 입력받아 해당하는 단의 구구단을 출력하시오
		// 로직 생각하기
		// 메소드로 처리할 부분
		// 1. 구구단 출력
		// main에서 처리할 부분
		// 1. 단 입력하기
		System.out.println("출력하고 싶은 구구단의 단을 입력하세요");
		int input = System.in.read()-'0';
		sFactor(input);
		
		

	}
}
