package test;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner input=new Scanner(System.in);
		int A = input.nextInt();
		int B = input.nextInt();
		int arr[] = new int [A];
		
		for (int i = 0; i < arr.length;i++) {
			arr[i] = i;
		}
		
		for (int i = 0; i < B; i++) {
			int C = input.nextInt();
			int D = input.nextInt();
			int temp = arr[C-1];
			arr[C-1] = arr[D-1];
			arr[D-1] = temp;
		}
		
		for (int i=0; i<arr.length; i++) {
			System.out.print(arr[i]+" ");
		}
	}
}