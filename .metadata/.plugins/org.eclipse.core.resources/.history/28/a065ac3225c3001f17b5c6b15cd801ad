package test;

public class Tast_ran {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num[][]=new int[3][3];
		
		/*
		 * for(int i=0;i<3;i++) { for(int j=0;j<3;j++) { num[i][j] =
		 * (int)(Math.random()*3+1); } }
		 */
		
		for(int i=0; i<num.length;i++) {
			System.out.println("i:"+i);
			num[0][i] = (int)(Math.random()*6+1);
			for(int j=0;j<i;j++) {
				System.out.println(num[0][i]+" "+num[0][j]);
				if(num[0][i]==num[0][j]) {//
					i--;//주 선택자를 고정하는 역할 i값이 마이너스 됨으로써 생성된 난수가 중복값이면 무한루프가 구현된다!
					break;//무한 루프가 될 것 같은데 왜 무한루프가 되지 않지?
				}
			}
		}
		int arr[]=num[0]; //여기에 배열 안의 값을 바로 저장 가능?

		for(int i=0;i<3;i++) {
			for(int j=0;j<3;j++) {
				System.out.print(num[i][j]);
			}
			System.out.println();
		}
		
	}

}
