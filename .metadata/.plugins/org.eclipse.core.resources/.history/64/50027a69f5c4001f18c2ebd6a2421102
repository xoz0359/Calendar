package test;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// 에디터 환경에서는 입력값의 마지막을 의미하는 EOF가 구현되지 않는다
		// BufferedReader를 활용할 때는 input.nextInt()==null일 때 EOF라고 인식하면 될듯!
		Scanner input=new Scanner(System.in);
		boolean sum = false;
		int sumn = 0;
		while (input.hasNext()) {
				
			int A = input.nextInt();
			sumn += A; 
			if (sum == true) {
				System.out.println(sumn);
				sumn = 0;
			}
			sum = !sum;
		}
	}
}