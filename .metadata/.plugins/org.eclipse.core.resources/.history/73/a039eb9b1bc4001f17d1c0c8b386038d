package day14;

public class MakaTest {

	public static void main(String[] args) {
		System.out.println("프로그램의 시작");
		
		Maka m1 = new Maka();//Maka는 래퍼런스 타입의 자료형으로 분류
		m1.color = "파랑";
		m1.makaWrite();
		
		Maka m2 = new Maka();
		m2.color = "빨강";
		m2.makaWrite();
		System.out.println("프로그램의 끝");
	}

}
