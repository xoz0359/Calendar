package test;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner input=new Scanner(System.in);
		boolean numb = true;
		while (numb) {
			numb = false;
			int A = input.nextInt();
			int B = input.nextInt();
			
			System.out.println(A+B);
			if (A==0 && B==0) {
				numb = false;
			}else {
				numb = true;
			}
		}
	}
}