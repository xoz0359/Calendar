package test;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner input=new Scanner(System.in);
		int num1 = 0, sum = 0;
		long A = input.nextInt();
		int A_2 = input.nextInt();
		for (int i=0; i<A_2; i++) {
			for (int j=0; j<2;j++) {
				num1 = 0;
				int B = input.nextInt();
				int C = input.nextInt();
				num1 = B*C;
			}
			sum += num1;
		}
		if (A==sum) {
			System.out.println("Yes");
		}else {
			System.out.println("No");
		}
	}
}