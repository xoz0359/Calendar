package test;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner input=new Scanner(System.in);
		boolean sum = false;
		int sumn = 0;
		while (input.hasNext()) {
				
			int A = input.nextInt();
			sumn += A; 
			if (sum == true) {
				System.out.println(sumn);
				sumn = 0;
				input.nextLine();
			}
			sum = !sum;
		}
	}
}