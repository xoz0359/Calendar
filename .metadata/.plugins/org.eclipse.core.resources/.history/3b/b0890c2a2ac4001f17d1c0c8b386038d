package test;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner input=new Scanner(System.in);
		int A = input.nextInt();
		for (int i=1;i<=A;i++) {
			for (int j=A; j>=1;j--) {
				if (j<=i) {
					System.out.print("*");
				}
				else {
					System.out.print(" ");
				}
				
			}
			System.out.println();
		}

	}
}