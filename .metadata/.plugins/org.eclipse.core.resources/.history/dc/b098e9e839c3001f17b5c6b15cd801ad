package day13;

public class Day13_04 {

	public static int getMaxNum(int num1, int num2) {
		int max = num1>num2?num1:num2;
		return max;
	}
	public static void main(String[] args) {
		
		int num1 = 10;
		int num2 = 20;
		
		int max = num1>num2?num1:num2;
		System.out.println("큰 수:"+max);
		max = getMaxNum(num1, num2);

	}

}
